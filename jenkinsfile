pipeline {
        agent any

triggers {
        githubPush()
}

        stages {

                stage (' connect and pull code from github ' ) {
                steps{
                 git branch : 'devlop',
                 url:'https://github.com/janakdasari-art/website-new.git'
                 echo " pulled code"
                }
                }

        stage('custom message'){
        steps{
                sh '''
                echo "pulled code"
                pwd
                '''
                }
        }
}
}

